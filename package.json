{
  "name": "lumina-studio-pro-v3.0",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "VITE_ENV=staging vite build",
    "build:prod": "VITE_ENV=production vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules package-lock.json .vite dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "size-check": "node scripts/check-bundle-size.js",
    "size-check-phase2": "PHASE=2 node scripts/check-bundle-size.js",
    "analyze": "vite build && npx vite-bundle-visualizer",
    "test": "vitest",
    "test:ci": "node --max-old-space-size=8192 --expose-gc node_modules/.bin/vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:load": "k6 run test/load/stress-test.js",
    "security:scan": "npm audit",
    "deps:check": "npx depcheck --skip-missing",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fontsource/roboto": "^5.2.9",
    "@material/material-color-utilities": "^0.3.0",
    "@mediapipe/tasks-vision": "0.10.8",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@sentry/react": "^10.29.0",
    "jszip": "^3.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@cypress/vite-dev-server": "^7.0.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.0",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "cypress": "^15.7.1",
    "eslint": "^9.17.0",
    "globals": "^16.5.0",
    "jsdom": "^23.0.1",
    "k6": "^0.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.15"
  }
}
